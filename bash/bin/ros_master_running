#!/usr/bin/env python2

import os
import sys
import socket
import xmlrpclib

ros_master_proxy = xmlrpclib.ServerProxy(os.environ['ROS_MASTER_URI'])
try:
    ros_master_proxy.getSystemState("/script/ros_master_test")
    sys.exit(0)
except socket.error as e:
    # If the master is unavaialble we get a scoket error.
    sys.exit(1)
